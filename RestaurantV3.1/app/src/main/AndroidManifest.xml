<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.jakkit.restaurant" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk android:minSdkVersion="8" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/restaurant"
        android:label="RestaurantV3"
        android:theme="@style/AppTheme" >
        <activity
            android:name="project.jakkit.restaurant.MainActivity"
            android:label="RestaurantV3"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="project.jakkit.restaurant.IndexMain"
            android:label="RestaurantV3" >
        </activity>
        <activity
            android:name="project.jakkit.restaurant.TableActivity"
            android:label="RestaurantV3" >
        </activity>
        <activity
            android:name="project.jakkit.restaurant.OrderActivity"
            android:label="RestaurantV3" >
        </activity>
        <activity
            android:name="project.jakkit.restaurant.ListOrderActivity"
            android:label="RestaurantV3" >
        </activity>
    </application>

</manifest>
